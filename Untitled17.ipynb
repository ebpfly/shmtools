     "data": {
      "application/javascript": [
       "\n",
       "// Remove existing panels\n",
       "document.querySelectorAll('[id*=\"shm-\"]').forEach(el => el.remove());\n",
       "\n",
       "// Generate insert commands (no external API calls)\n",
       "function generateInsert(codeKey) {\n",
       "    console.log('Generating insert for:', codeKey);\n",
       "    \n",
       "    const insertCmd = `insert_shm_code(shm_codes[\"${codeKey}\"])`;\n",
       "    \n",
       "    // Copy to clipboard\n",
       "    try {\n",
       "        navigator.clipboard.writeText(insertCmd);\n",
       "        console.log('âœ… Copied to clipboard:', insertCmd);\n",
       "    } catch(e) {\n",
       "        console.log('Command to run:', insertCmd);\n",
       "    }\n",
       "    \n",
       "    // Display the command prominently\n",
       "    const display = document.getElementById('shm-command-display');\n",
       "    display.innerHTML = `\n",
       "        <div style=\"background: #d4edda; padding: 12px; border-radius: 5px; margin: 10px; border-left: 4px solid #28a745;\">\n",
       "            <strong>ðŸ“‹ Run this command in the next cell:</strong><br>\n",
       "            <code style=\"font-size: 13px; background: white; padding: 8px; border-radius: 3px; display: block; margin-top: 8px; font-family: monospace; word-break: break-all;\">\n",
       "                ${insertCmd}\n",
       "            </code>\n",
       "            <small style=\"color: #666; margin-top: 5px; display: block;\">\n",
       "                ðŸ’¡ Command copied to clipboard - just paste and run!\n",
       "            </small>\n",
       "        </div>\n",
       "    `;\n",
       "}\n",
       "\n",
       "// Create the GUI panel\n",
       "const panel = document.createElement('div');\n",
       "panel.id = 'shm-clean-panel';\n",
       "panel.style.cssText = `\n",
       "    position: fixed;\n",
       "    top: 60px;\n",
       "    right: 20px;\n",
       "    background: white;\n",
       "    border: 2px solid #007bff;\n",
       "    border-radius: 8px;\n",
       "    box-shadow: 0 4px 20px rgba(0,0,0,0.15);\n",
       "    z-index: 1000;\n",
       "    width: 320px;\n",
       "    font-family: Arial, sans-serif;\n",
       "    max-height: 600px;\n",
       "    overflow-y: auto;\n",
       "`;\n",
       "\n",
       "// Build the HTML content\n",
       "panel.innerHTML = `\n",
       "    <div style=\"background: #007bff; color: white; padding: 12px 16px; font-weight: bold; border-radius: 6px 6px 0 0; cursor: move;\">\n",
       "        ðŸ”§ SHM Functions\n",
       "    </div>\n",
       "    \n",
       "    <div id=\"shm-command-display\">\n",
       "        <div style=\"padding: 15px; text-align: center; color: #666; font-style: italic;\">\n",
       "            Click any function below to generate insertion command\n",
       "        </div>\n",
       "    </div>\n",
       "    \n",
       "    <div style=\"padding: 10px;\">\n",
       "        <!-- Spectral Analysis -->\n",
       "        <div style=\"background: #f8f9fa; padding: 8px 12px; margin-bottom: 8px; font-weight: bold; color: #495057; font-size: 12px; border-left: 4px solid #007bff;\">\n",
       "            SPECTRAL ANALYSIS\n",
       "        </div>\n",
       "        <button onclick=\"generateInsert('psd_welch')\" style=\"width: 100%; padding: 10px; margin: 3px 0; background: #007bff; color: white; border: none; border-radius: 4px; cursor: pointer; font-size: 13px;\">\n",
       "            PSD Welch\n",
       "        </button>\n",
       "        <button onclick=\"generateInsert('spectrogram')\" style=\"width: 100%; padding: 10px; margin: 3px 0; background: #007bff; color: white; border: none; border-radius: 4px; cursor: pointer; font-size: 13px;\">\n",
       "            Spectrogram\n",
       "        </button>\n",
       "        \n",
       "        <!-- Time Series -->\n",
       "        <div style=\"background: #f8f9fa; padding: 8px 12px; margin: 15px 0 8px 0; font-weight: bold; color: #495057; font-size: 12px; border-left: 4px solid #007bff;\">\n",
       "            TIME SERIES\n",
       "        </div>\n",
       "        <button onclick=\"generateInsert('ar_model')\" style=\"width: 100%; padding: 10px; margin: 3px 0; background: #007bff; color: white; border: none; border-radius: 4px; cursor: pointer; font-size: 13px;\">\n",
       "            AR Model\n",
       "        </button>\n",
       "        \n",
       "        <!-- Outlier Detection -->\n",
       "        <div style=\"background: #f8f9fa; padding: 8px 12px; margin: 15px 0 8px 0; font-weight: bold; color: #495057; font-size: 12px; border-left: 4px solid #007bff;\">\n",
       "            OUTLIER DETECTION\n",
       "        </div>\n",
       "        <button onclick=\"generateInsert('learn_pca')\" style=\"width: 100%; padding: 10px; margin: 3px 0; background: #007bff; color: white; border: none; border-radius: 4px; cursor: pointer; font-size: 13px;\">\n",
       "            Learn PCA\n",
       "        </button>\n",
       "        <button onclick=\"generateInsert('score_pca')\" style=\"width: 100%; padding: 10px; margin: 3px 0; background: #007bff; color: white; border: none; border-radius: 4px; cursor: pointer; font-size: 13px;\">\n",
       "            Score PCA\n",
       "        </button>\n",
       "        \n",
       "        <!-- Filtering -->\n",
       "        <div style=\"background: #f8f9fa; padding: 8px 12px; margin: 15px 0 8px 0; font-weight: bold; color: #495057; font-size: 12px; border-left: 4px solid #007bff;\">\n",
       "            FILTERING\n",
       "        </div>\n",
       "        <button onclick=\"generateInsert('bandpass')\" style=\"width: 100%; padding: 10px; margin: 3px 0; background: #007bff; color: white; border: none; border-radius: 4px; cursor: pointer; font-size: 13px;\">\n",
       "            Bandpass Filter\n",
       "        </button>\n",
       "    </div>\n",
       "`;\n",
       "\n",
       "// Make the function global so buttons can call it\n",
       "window.generateInsert = generateInsert;\n",
       "\n",
       "// Make it draggable\n",
       "let isDragging = false, dragOffset = {x: 0, y: 0};\n",
       "const header = panel.querySelector('div');\n",
       "header.onmousedown = e => {\n",
       "    isDragging = true;\n",
       "    dragOffset.x = e.clientX - panel.offsetLeft;\n",
       "    dragOffset.y = e.clientY - panel.offsetTop;\n",
       "};\n",
       "document.onmousemove = e => {\n",
       "    if (isDragging) {\n",
       "        panel.style.left = (e.clientX - dragOffset.x) + 'px';\n",
       "        panel.style.top = (e.clientY - dragOffset.y) + 'px';\n",
       "        panel.style.right = 'auto';\n",
       "    }\n",
       "};\n",
       "document.onmouseup = () => isDragging = false;\n",
       "\n",
       "// Add to page\n",
       "document.body.appendChild(panel);\n",
       "\n",
       "console.log('âœ… Clean SHM GUI loaded successfully');\n",
       "console.log('ðŸ’¡ Click buttons to generate insertion commands');\n"
      ],
      "text/plain": [
       "<IPython.core.display.Javascript object>"
      ]
