   "outputs": [
    {
     "data": {
      "application/javascript": [
       "\n",
       "// Remove existing panels\n",
       "document.querySelectorAll('[id*=\"shm-dropdown\"]').forEach(el => el.remove());\n",
       "\n",
       "// JupyterLab-specific insertion function\n",
       "function insertCode(code) {\n",
       "    console.log('Attempting to insert code in JupyterLab:', code);\n",
       "    \n",
       "    try {\n",
       "        // Method 1: Try to find active notebook panel and kernel\n",
       "        const notebookPanels = document.querySelectorAll('.jp-NotebookPanel');\n",
       "        console.log('Found notebook panels:', notebookPanels.length);\n",
       "        \n",
       "        if (notebookPanels.length > 0) {\n",
       "            // Try to execute via the Python kernel using a different approach\n",
       "            // Create a temporary script element to execute Python\n",
       "            const script = document.createElement('script');\n",
       "            script.type = 'text/python';\n",
       "            script.textContent = `_shm_insert_code(\"\"\"${code}\"\"\")`;\n",
       "            document.head.appendChild(script);\n",
       "            document.head.removeChild(script);\n",
       "            \n",
       "            console.log('✅ Attempted execution via script injection');\n",
       "            return true;\n",
       "        }\n",
       "        \n",
       "        // Method 2: Try to find and use JupyterLab's command system\n",
       "        if (window._JUPYTERLAB) {\n",
       "            console.log('Found JupyterLab globals');\n",
       "            \n",
       "            // Try to dispatch a command to insert code\n",
       "            const event = new CustomEvent('jupyterlab:insert-code', {\n",
       "                detail: { code: code }\n",
       "            });\n",
       "            document.dispatchEvent(event);\n",
       "            \n",
       "            console.log('✅ Dispatched JupyterLab insert event');\n",
       "            return true;\n",
       "        }\n",
       "        \n",
       "        // Method 3: Use the global Python function directly via fetch\n",
       "        fetch('/api/kernels')\n",
       "            .then(response => response.json())\n",
       "            .then(kernels => {\n",
       "                if (kernels.length > 0) {\n",
       "                    const kernelId = kernels[0].id;\n",
       "                    return fetch(`/api/kernels/${kernelId}/execute`, {\n",
       "                        method: 'POST',\n",
       "                        headers: { 'Content-Type': 'application/json' },\n",
       "                        body: JSON.stringify({\n",
       "                            code: `_shm_insert_code(\"\"\"${code}\"\"\")`\n",
       "                        })\n",
       "                    });\n",
       "                }\n",
       "            })\n",
       "            .then(() => console.log('✅ Code sent via kernel API'))\n",
       "            .catch(e => console.log('❌ Kernel API failed:', e));\n",
       "            \n",
       "        return true;\n",
       "        \n",
       "    } catch (e) {\n",
       "        console.log('❌ All JupyterLab methods failed:', e);\n",
       "        \n",
       "        // Last resort: copy to clipboard\n",
       "        try {\n",
       "            navigator.clipboard.writeText(code);\n",
       "            console.log('⚠️ Copied to clipboard as fallback');\n",
       "            return false;\n",
       "        } catch (e2) {\n",
       "            console.log('❌ Even clipboard failed');\n",
       "            return false;\n",
       "        }\n",
       "    }\n",
       "}\n",
       "\n",
       "const funcs = {\n",
       "    'Spectral': [\n",
       "        {name: 'PSD Welch', code: 'frequencies, psd = shmtools.psd_welch(x=data, fs=fs, nperseg=1024)'},\n",
       "        {name: 'Spectrogram', code: 'f, t, Sxx = shmtools.spectrogram(x=data, fs=fs, nperseg=1024)'}\n",
       "    ],\n",
       "    'Time Series': [\n",
       "        {name: 'AR Model', code: 'features, residuals = shmtools.ar_model(X=data, ar_order=15)'}\n",
       "    ],\n",
       "    'Outlier Detection': [\n",
       "        {name: 'Learn PCA', code: 'model = shmtools.learn_pca(X=features, per_var=0.95)'},\n",
       "        {name: 'Score PCA', code: 'scores, outliers = shmtools.score_pca(Y=test_data, model=model)'}\n",
       "    ],\n",
       "    'Filtering': [\n",
       "        {name: 'Bandpass', code: 'filtered = shmtools.bandpass_filter(x=signal, lowcut=10, highcut=100, fs=fs)'}\n",
       "    ]\n",
       "};\n",
       "\n",
       "// Create floating panel\n",
       "const panel = document.createElement('div');\n",
       "panel.id = 'shm-dropdown-panel';\n",
       "panel.style.cssText = `\n",
       "    position: fixed;\n",
       "    top: 60px;\n",
       "    right: 20px;\n",
       "    background: white;\n",
       "    border: 2px solid #007bff;\n",
       "    border-radius: 8px;\n",
       "    box-shadow: 0 4px 20px rgba(0,0,0,0.15);\n",
       "    z-index: 1000;\n",
       "    width: 280px;\n",
       "    font-family: Arial, sans-serif;\n",
       "`;\n",
       "\n",
       "// Header\n",
       "const header = document.createElement('div');\n",
       "header.innerHTML = '🔧 SHM Functions';\n",
       "header.style.cssText = `\n",
       "    background: #007bff;\n",
       "    color: white;\n",
       "    padding: 12px 16px;\n",
       "    font-weight: bold;\n",
       "    font-size: 14px;\n",
       "    border-radius: 6px 6px 0 0;\n",
       "    cursor: move;\n",
       "`;\n",
       "\n",
       "// Make draggable\n",
       "let isDragging = false, dragOffset = {x: 0, y: 0};\n",
       "header.onmousedown = e => {\n",
       "    isDragging = true;\n",
       "    dragOffset.x = e.clientX - panel.offsetLeft;\n",
       "    dragOffset.y = e.clientY - panel.offsetTop;\n",
       "};\n",
       "document.onmousemove = e => {\n",
       "    if (isDragging) {\n",
       "        panel.style.left = (e.clientX - dragOffset.x) + 'px';\n",
       "        panel.style.top = (e.clientY - dragOffset.y) + 'px';\n",
       "        panel.style.right = 'auto';\n",
       "    }\n",
       "};\n",
       "document.onmouseup = () => isDragging = false;\n",
       "\n",
       "panel.appendChild(header);\n",
       "\n",
       "// Content\n",
       "const content = document.createElement('div');\n",
       "content.style.cssText = 'max-height: 400px; overflow-y: auto;';\n",
       "\n",
       "Object.keys(funcs).forEach(category => {\n",
       "    const catHeader = document.createElement('div');\n",
       "    catHeader.textContent = category;\n",
       "    catHeader.style.cssText = `\n",
       "        padding: 10px 16px;\n",
       "        background: #f8f9fa;\n",
       "        font-weight: bold;\n",
       "        color: #495057;\n",
       "        font-size: 12px;\n",
       "        border-bottom: 1px solid #dee2e6;\n",
       "    `;\n",
       "    content.appendChild(catHeader);\n",
       "    \n",
       "    funcs[category].forEach(func => {\n",
       "        const item = document.createElement('div');\n",
       "        item.textContent = func.name;\n",
       "        item.style.cssText = `\n",
       "            padding: 10px 16px;\n",
       "            cursor: pointer;\n",
       "            border-bottom: 1px solid #f1f3f4;\n",
       "            transition: background-color 0.2s;\n",
       "        `;\n",
       "        \n",
       "        item.onmouseover = () => item.style.backgroundColor = '#e3f2fd';\n",
       "        item.onmouseout = () => item.style.backgroundColor = 'transparent';\n",
       "        item.onclick = () => {\n",
       "            insertCode(func.code);\n",
       "        };\n",
       "        \n",
       "        content.appendChild(item);\n",
       "    });\n",
       "});\n",
       "\n",
       "panel.appendChild(content);\n",
       "document.body.appendChild(panel);\n",
       "\n",
       "console.log('✅ SHM Extension loaded - floating panel created');\n",
       "console.log('Available objects:', {\n",
       "    'window.IPython': typeof window.IPython,\n",
       "    'window.Jupyter': typeof window.Jupyter,\n",
       "    'Jupyter': typeof Jupyter\n",
       "});\n",
       "        "
      ],
      "text/plain": [
       "<IPython.core.display.Javascript object>"
      ]
     },
