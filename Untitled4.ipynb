   "outputs": [
    {
     "data": {
      "application/javascript": [
       "\n",
       "console.log('=== DEBUG: Starting SHM Extension ===');\n",
       "\n",
       "// Remove existing panels\n",
       "document.querySelectorAll('[id*=\"shm-dropdown\"]').forEach(el => el.remove());\n",
       "\n",
       "// Debug function to insert code\n",
       "function insertCodeIntoCell(code) {\n",
       "    console.log('=== DEBUG: insertCodeIntoCell called ===');\n",
       "    console.log('Code to insert:', code);\n",
       "    \n",
       "    // Check Jupyter availability\n",
       "    console.log('Jupyter object:', typeof Jupyter);\n",
       "    if (typeof Jupyter !== 'undefined') {\n",
       "        console.log('Jupyter.notebook:', typeof Jupyter.notebook);\n",
       "        if (Jupyter.notebook) {\n",
       "            console.log('Jupyter.notebook methods:', Object.keys(Jupyter.notebook));\n",
       "        }\n",
       "    }\n",
       "    \n",
       "    try {\n",
       "        if (typeof Jupyter === 'undefined' || !Jupyter.notebook) {\n",
       "            console.log('‚ùå Jupyter not available');\n",
       "            return false;\n",
       "        }\n",
       "        \n",
       "        console.log('‚úÖ Jupyter available, proceeding with insertion...');\n",
       "        \n",
       "        // Get current state\n",
       "        const selectedIndex = Jupyter.notebook.get_selected_index();\n",
       "        const selectedCell = Jupyter.notebook.get_selected_cell();\n",
       "        \n",
       "        console.log('Selected index:', selectedIndex);\n",
       "        console.log('Selected cell type:', selectedCell ? selectedCell.cell_type : 'null');\n",
       "        console.log('Selected cell text:', selectedCell ? selectedCell.get_text() : 'null');\n",
       "        \n",
       "        let targetCell;\n",
       "        \n",
       "        if (selectedCell && selectedCell.cell_type === 'code' && selectedCell.get_text().trim() === '') {\n",
       "            console.log('Using empty current cell');\n",
       "            targetCell = selectedCell;\n",
       "        } else {\n",
       "            console.log('Creating new cell below');\n",
       "            targetCell = Jupyter.notebook.insert_cell_below('code');\n",
       "            Jupyter.notebook.select_next();\n",
       "        }\n",
       "        \n",
       "        console.log('Target cell:', targetCell);\n",
       "        \n",
       "        if (targetCell) {\n",
       "            targetCell.set_text(code);\n",
       "            console.log('‚úÖ Code set in cell');\n",
       "            \n",
       "            // Try to focus\n",
       "            if (targetCell.focus_editor) {\n",
       "                targetCell.focus_editor();\n",
       "                console.log('‚úÖ Editor focused');\n",
       "            }\n",
       "            \n",
       "            return true;\n",
       "        } else {\n",
       "            console.log('‚ùå No target cell created');\n",
       "            return false;\n",
       "        }\n",
       "        \n",
       "    } catch (e) {\n",
       "        console.log('‚ùå Error during insertion:', e);\n",
       "        console.log('Error stack:', e.stack);\n",
       "        return false;\n",
       "    }\n",
       "}\n",
       "\n",
       "// Test functions\n",
       "const testFuncs = {\n",
       "    'Test': [\n",
       "        {\n",
       "            name: 'Simple Test',\n",
       "            code: '# This is a test\\nprint(\"Hello from SHM!\")'\n",
       "        },\n",
       "        {\n",
       "            name: 'PSD Welch',\n",
       "            code: 'frequencies, psd = shmtools.psd_welch(x=data, fs=fs, nperseg=1024)'\n",
       "        }\n",
       "    ]\n",
       "};\n",
       "\n",
       "// Create panel\n",
       "const panel = document.createElement('div');\n",
       "panel.id = 'shm-debug-panel';\n",
       "panel.style.cssText = `\n",
       "    position: fixed;\n",
       "    top: 60px;\n",
       "    right: 20px;\n",
       "    background: white;\n",
       "    border: 2px solid #ff6b6b;\n",
       "    border-radius: 8px;\n",
       "    box-shadow: 0 4px 20px rgba(0,0,0,0.15);\n",
       "    z-index: 1000;\n",
       "    width: 300px;\n",
       "    font-family: Arial, sans-serif;\n",
       "`;\n",
       "\n",
       "// Header\n",
       "const header = document.createElement('div');\n",
       "header.innerHTML = 'üêõ SHM DEBUG';\n",
       "header.style.cssText = `\n",
       "    background: #ff6b6b;\n",
       "    color: white;\n",
       "    padding: 12px 16px;\n",
       "    font-weight: bold;\n",
       "    font-size: 14px;\n",
       "    border-radius: 6px 6px 0 0;\n",
       "    cursor: move;\n",
       "`;\n",
       "\n",
       "panel.appendChild(header);\n",
       "\n",
       "// Content\n",
       "const content = document.createElement('div');\n",
       "content.style.cssText = 'padding: 10px;';\n",
       "\n",
       "Object.keys(testFuncs).forEach(category => {\n",
       "    const catHeader = document.createElement('div');\n",
       "    catHeader.textContent = category;\n",
       "    catHeader.style.cssText = `\n",
       "        padding: 8px 0;\n",
       "        font-weight: bold;\n",
       "        color: #333;\n",
       "        border-bottom: 1px solid #eee;\n",
       "    `;\n",
       "    content.appendChild(catHeader);\n",
       "    \n",
       "    testFuncs[category].forEach(func => {\n",
       "        const item = document.createElement('button');\n",
       "        item.textContent = func.name;\n",
       "        item.style.cssText = `\n",
       "            display: block;\n",
       "            width: 100%;\n",
       "            padding: 10px;\n",
       "            margin: 5px 0;\n",
       "            cursor: pointer;\n",
       "            background: #f8f9fa;\n",
       "            border: 1px solid #ddd;\n",
       "            border-radius: 4px;\n",
       "            transition: background-color 0.2s;\n",
       "        `;\n",
       "        \n",
       "        item.onmouseover = () => item.style.backgroundColor = '#e3f2fd';\n",
       "        item.onmouseout = () => item.style.backgroundColor = '#f8f9fa';\n",
       "        \n",
       "        item.onclick = () => {\n",
       "            console.log('=== CLICK: ' + func.name + ' ===');\n",
       "            const success = insertCodeIntoCell(func.code);\n",
       "            console.log('Insertion result:', success);\n",
       "        };\n",
       "        \n",
       "        content.appendChild(item);\n",
       "    });\n",
       "});\n",
       "\n",
       "panel.appendChild(content);\n",
       "document.body.appendChild(panel);\n",
       "\n",
       "console.log('=== DEBUG: Panel created ===');\n"
      ],
      "text/plain": [
       "<IPython.core.display.Javascript object>"
      ]
